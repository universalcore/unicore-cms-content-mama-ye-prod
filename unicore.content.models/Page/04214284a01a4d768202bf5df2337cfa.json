  "description": "\u0627\u0644\u0628\u062f\u0621 \u0628\u0627\u0644\u0623\u0637\u0639\u0645\u0629 \u0627\u0644\u0635\u0644\u0628\u0629", 
  "language": "ara_AE", 
  "title": "\u0627\u0644\u0634\u0647\u0631 \u0627\u0644\u0633\u0627\u062f\u0633 \u0645\u0646 \u0627\u0644\u0639\u0645\u0631", 
  "primary_category": "6f7ff2ae06d64bb8bc905213511a5fd8", 
  "created_at": "2015-04-16T11:03:09+00:00", 
  "featured_in_category": true, 
  "modified_at": "2015-12-11T15:06:44.617661+00:00", 
  "_version": {
    "package_version": "1.0.1", 
